    <script>

    //tim kiem trang chu suggest
    function checksuggest(){
        $('.timkiem').css("display","none");
    }
    function CheckPostSearch(e) {
        $('.timkiem').css("display","block");
          if (e.keyCode == 13) {
            postSearch();
          };
          $("#tbSearch").show().focus();
            keyword=$("#tbSearch").val();
            console.log(keyword);
            $.ajax({
              type:"POST",
              data:{keyword:keyword},
              url:'/',
              success:function(data){
                var suggest=document.getElementsByClassName('pro-suggest')[0];
                var content=""
                for(var i=0; i<data.count;i++){
                    content+='<li>' +
                    '<span class="image-product-suggest"><img src=/'+data.product[i].imagedefault[0].originalname+'></span>' +
                    '<div class="detail-suggest">' +
                    '<span class="name-product-suggest">' +
                    '<a href=/tim-kiem-san-pham/'+data.product[i].nameseo +'>'+data.product[i].name +"</a></span>"+
                    '<b class="price-product-suggest">' +data.product[i].pricesale +" ₫</b>"+
                    '</div>'
                    '</li>';
                }
                suggest.innerHTML="";
                suggest.innerHTML=content;
              }
            });
          }
    function postSearch() {
      var weburl = "localhost://3000/";
      // if (document.URL.indexOf("www.") > -1) window.location = document.URL.replace("www.", "");
        $("#tbSearch").show().focus();
        if ($("#tbSearch").val() != ""){
          location.href ="/tim-kiem/keywords=" + $("#tbSearch").val();
        //  window.location = "/tim-kiem/keywords=" + $("#tbSearch").val();
        }
    };
    function removeAccents(strAccents){
      strAccents = strAccents.split('');
      strAccentsOut = new Array();
      strAccentsLen = strAccents.length;
      var accents = 'ÀÁÂÃÄÅàáâãäåạÒÓÔÕÕÖØòóôõöộọøếÈÉÊËèéêëễðÇçÐÌÍÎÏìíîïÙÚÛÜùúûüÑñŠšŸÿýŽž';
      var accentsOut = ['A','A','A','A','A','A','a','a','a','a','a','a','O','O','O','O','O','O','O','o','o','o','o','o','o','E','E','E','E','e','e','e','e','e','C','c','D','I','I','I','I','i','i','i','i','U','U','U','U','u','u','u','u','N','n','S','s','Y','y','y','Z','z'];
      for (var y = 0; y < strAccentsLen; y++) {
          if (accents.indexOf(strAccents[y]) != -1) {
              strAccentsOut[y] = accentsOut[accents.indexOf(strAccents[y])];
          }
          else
              strAccentsOut[y] = strAccents[y];
      }
      strAccentsOut = strAccentsOut.join('');
  	   strAccentsOut = strAccentsOut.toLowerCase();
      return strAccentsOut;
    }
    function numberWithCommas(x) {
      return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
    };
    $(document).ready(function(){
      var commas=document.getElementsByClassName('commas');
      for(var i=0;i<commas.length;i++){
        commas[i].innerHTML=numberWithCommas(commas[i].innerHTML);
      }
    })
